import lombok.extern.slf4j.Slf4j;

@Slf4j
@Service 
public class BootCoinServiceImpl implements BootCoinService {
	
	@Override
	public String processCreateBootCoinPurse(BootCoin transaction) throws InterruptedException {


		List<BankAccountResponse> bankAccountrelacionado = new ArrayList<>();
		
		if(transaction.getNumberDebitCard() != null ) {
			bankAccountrelacionado = this.bankAccountApiClient.getDebitCardbyCustomerNumberCreditCard(yanki.getNumberDebitCard());	
		}
		
	
	}

}
